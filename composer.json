{
  "name": "janwebdev/omnipay-cardinity",
  "description": "Cardinity gateway for Omnipay library",
  "keywords": [
    "omnipay",
    "cardinity",
    "payment",
    "gateway",
    "integration"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Jan Rogozinsky",
      "email": "janwebdev@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Omnipay\\Cardinity\\": "src/",
      "Omnipay\\Cardinity\\Tests\\": "tests/"
    }
  },
  "require": {
    "ext-json": "*",
    "php": "^7.4|^8.0|^8.1",
    "league/omnipay": "^3"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "phpro/grumphp": "^1.3",
    "omnipay/tests": "^3|^4",
    "squizlabs/php_codesniffer": "^3.5",
    "phpstan/phpstan": "^0.12.81"
  },
  "scripts": {
    "test": "./vendor/bin/grumphp run"
  },
  "extra": {
    "branch-alias": {
      "dev-main": "2.x-dev"
    }
  },
  "config": {
    "allow-plugins": {
      "phpro/grumphp": true
    }
  }
}
